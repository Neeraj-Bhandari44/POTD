
class Solution
{
    public:
    //Function to find the first non-repeating character in a string.
    char nonrepeatingCharacter(string S)
    {
       unordered_map<int,int>mp;
       for(int i=0;i<S.length();i++)
       {
           mp[S[i]]++;
       }
       for(int i=0;i<S.length();i++)
       {
           if(mp[S[i]]==1)
           {
               return S[i];
           }
       }
       return '$';
    }

};
